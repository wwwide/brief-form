{
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": ["ES5", "ES6", "ES7", "ES2017", "ES2018", "ES2019", "ES2020", "ES2021", "ESNext", "DOM"],
    "baseUrl": "./src",
    "paths": {}
  },
  "exclude": ["node_modules", "debug"]
}
